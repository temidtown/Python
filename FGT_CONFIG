        set status down 

        set netbios-forward disable 

        set wins-ip 0.0.0.0 

        set type physical 

        set netflow-sampler disable 

        set sflow-sampler disable 

        set  scan-botnet-connections disable 

        set src-check enable 

        set sample-rate 2000 

        set polling-interval 20 

