#edit 1 

#set start-ipx.x.x.x 

#set end-ipx.x.x.x 

 

#edit 2 

        set interface "dmz" 

        config ip-range 

            edit 1 

                set start-ip x.x.x.x 

                set end-ip x.x.x.x 

            next 

        end 

 

