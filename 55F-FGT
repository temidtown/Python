config switch-controller stp-settings
    set name ''
    set revision 0
    set hello-time 2
    set forward-time 15
    set max-age 20
    set max-hops 20
end
config switch-controller storm-control
    set rate 500
    set unknown-unicast enable
    set unknown-multicast disable
    set broadcast enable
end
config switch-controller global
    set mac-aging-interval 300
    set allow-multiple-interfaces disable
    set https-image-push disable
    set mac-retention-period 24
    set default-virtual-switch-vlan ''
    set log-mac-limit-violations disable
end
config switch-controller switch-log
    set status enable
    set severity critical
end
config switch-controller igmp-snooping
    set aging-time 300
    set flood-unknown-multicast disable
end
config switch-controller sflow
    set collector-ip 0.0.0.0
    set collector-port 6343
end
config switch-controller network-monitor-settings
    set network-monitoring disable
end
config endpoint-control settings
    set forticlient-reg-key-enforce disable
    set forticlient-reg-timeout 7
    set download-location fortiguard
    set forticlient-offline-grace disable
    set forticlient-keepalive-interval 60
    set forticlient-sys-update-interval 720
    set forticlient-avdb-update-interval 8
    set forticlient-warning-interval 1
    set forticlient-user-avatar enable
    set forticlient-dereg-unsupported-client enable
    set forticlient-ems-rest-api-call-timeout 5000
end
config endpoint-control forticlient-ems
end
config endpoint-control profile
    edit "default"
        config forticlient-winmac-settings
            set forticlient-registration-compliance-action warning
            set forticlient-ems-compliance disable
            set forticlient-security-posture disable
            set forticlient-system-compliance enable
            set forticlient-system-compliance-action warning
            set forticlient-minimum-software-version disable
            set forticlient-log-upload enable
            set forticlient-log-upload-level traffic vulnerability event
            set forticlient-log-upload-server ''
            set forticlient-vuln-scan enable
            set forticlient-vuln-scan-compliance-action warning
            set forticlient-vuln-scan-enforce high
            set forticlient-vuln-scan-enforce-grace 1
            set forticlient-vuln-scan-exempt disable
        end
        config forticlient-android-settings
            set forticlient-wf disable
            set disable-wf-when-protected enable
            set forticlient-vpn-provisioning disable
        end
        config forticlient-ios-settings
            set forticlient-wf disable
            set disable-wf-when-protected enable
            set client-vpn-provisioning disable
            set distribute-configuration-profile disable
        end
        set description ''
        set replacemsg-override-group ''
    next
end
config ips custom
end
