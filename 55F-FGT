    set packet-log-history 1
    set packet-log-post-attack 0
    set packet-log-memory 256
    set ips-packet-quota 0
end
config wireless-controller setting
    set account-id ''
    set country US
    set duplicate-ssid disable
    set fapc-compatibility disable
