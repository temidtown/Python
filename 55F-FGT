config firewall service group
    edit "Email Access"
        set member "DNS" "IMAP" "IMAPS" "POP3" "POP3S" "SMTP" "SMTPS"
        set proxy disable
        set comment ''
        set color 0
    next
    edit "Web Access"
        set member "DNS" "HTTP" "HTTPS"
        set proxy disable
        set comment ''
        set color 0
    next
    edit "Windows AD"
        set member "DCE-RPC" "DNS" "KERBEROS" "LDAP" "LDAP_UDP" "SAMBA" "SMB"
        set proxy disable
        set comment ''
        set color 0
    next
    edit "Exchange Server"
        set member "DCE-RPC" "DNS" "HTTPS"
        set proxy disable
        set comment ''
        set color 0
    next
