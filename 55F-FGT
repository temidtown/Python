config antivirus profile
    edit "wifi-default"
        set comment "Default configuration for offloading WiFi traffic."
        set replacemsg-group ''
        set inspection-mode flow-based
        set ftgd-analytics disable
        set analytics-db disable
        set mobile-malware-db enable
        config http
            set options scan
            unset archive-block
            unset archive-log
            set emulator enable
            set outbreak-prevention disabled
        end
        config ftp
            set options scan
            unset archive-block
            unset archive-log
            set emulator enable
            set outbreak-prevention disabled
        end
        config imap
            set options scan
            unset archive-block
            unset archive-log
            set emulator enable
            set executables virus
            set outbreak-prevention disabled
        end
        config pop3
            set options scan
            unset archive-block
            unset archive-log
            set emulator enable
            set executables virus
            set outbreak-prevention disabled
        end
        config smtp
            set options scan
            unset archive-block
            unset archive-log
            set emulator enable
            set executables virus
            set outbreak-prevention disabled
        end
        config nntp
            unset options
            unset archive-block
            unset archive-log
            set emulator enable
            set outbreak-prevention disabled
        end
        config smb
            unset options
            unset archive-block
            unset archive-log
            set emulator enable
            set outbreak-prevention disabled
        end
        config nac-quar
            set infected none
            set log disable
        end
        set av-virus-log enable
        set av-block-log enable
        set extended-log disable
        set scan-mode full
    next
    edit "sniffer-profile"
        set comment "Scan files and monitor viruses."
        set replacemsg-group ''
