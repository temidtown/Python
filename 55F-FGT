        set ftgd-analytics disable
        set analytics-db disable
        set mobile-malware-db enable
        config http
            set options scan
            unset archive-block
            unset archive-log
            set emulator enable
            set outbreak-prevention disabled
        end
        config ftp
            set options scan
            unset archive-block
            unset archive-log
            set emulator enable
            set outbreak-prevention disabled
        end
        config imap
            set options scan
            unset archive-block
            unset archive-log
            set emulator enable
            set executables virus
            set outbreak-prevention disabled
        end
        config pop3
            set options scan
            unset archive-block
            unset archive-log
            set emulator enable
            set executables virus
            set outbreak-prevention disabled
        end
        config smtp
            set options scan
            unset archive-block
            unset archive-log
            set emulator enable
            set executables virus
            set outbreak-prevention disabled
        end
        config nntp
            unset options
            unset archive-block
            unset archive-log
            set emulator enable
            set outbreak-prevention disabled
        end
        config smb
            unset options
            unset archive-block
            unset archive-log
            set emulator enable
            set outbreak-prevention disabled
        end
        config nac-quar
            set infected none
            set log disable
        end
        set av-virus-log enable
        set av-block-log enable
        set extended-log disable
        set scan-mode full
    next
    edit "default"
        set comment "scan and delete virus"
        set replacemsg-group ''
        set inspection-mode proxy
        set ftgd-analytics disable
        set analytics-db disable
        set mobile-malware-db enable
        config http
            set options scan
            unset archive-block
            unset archive-log
            set emulator enable
            set outbreak-prevention disabled
            set content-disarm disable
        end
        config ftp
            set options scan
            unset archive-block
            unset archive-log
            set emulator enable
            set outbreak-prevention disabled
        end
        config imap
            set options scan
            unset archive-block
            unset archive-log
            set emulator enable
            set executables default
            set outbreak-prevention disabled
            set content-disarm disable
        end
        config  pop3
            set options scan
            unset archive-block
            unset archive-log
            set emulator enable
            set executables default
            set outbreak-prevention disabled
            set content-disarm disable
        end
        config smtp
            set options scan
            unset archive-block
            unset archive-log
            set emulator enable
            set executables default
            set outbreak-prevention disabled
            set content-disarm disable
        end
        config mapi
            unset options
            unset archive-block
            unset archive-log
            set emulator enable
            set executables default
            set outbreak-prevention disabled
        end
        config nntp
            set options scan
            unset archive-block
            unset archive-log
            set emulator enable
            set outbreak-prevention disabled
        end
        config nac-quar
            set infected none
            set log disable
        end
        config content-disarm
            set original-file-destination discard
            set office-macro enable
            set office-hylink enable
            set office-linked enable
            set office-embed enable
            set pdf-javacode enable
            set pdf-embedfile enable
            set pdf-hyperlink enable
            set pdf-act-gotor enable
            set pdf-act-launch enable
            set pdf-act-sound enable
            set pdf-act-movie enable
            set pdf-act-java enable
            set pdf-act-form enable
            set cover-page enable
            set detect-only disable
        end
        set av-virus-log enable
        set av-block-log enable
        set extended-log disable
    next
end
config spamfilter profile
    edit "sniffer-profile"
        set comment "Malware and phishing URL monitoring."
        set flow-based enable
        set replacemsg-group ''
        set spam-log enable
        set spam-filtering disable
        set external disable
        unset options
        config imap
            set log disable
        end
        config pop3
            set log disable
        end
        config smtp
            set log disable
        end
        config msn-hotmail
            set log disable
        end
        config gmail
            set log disable
        end
        set spam-bword-threshold 10
        unset spam-bword-table
        unset spam-bwl-table
        unset spam-mheader-table
        unset spam-rbl-table
        unset spam-iptrust-table
        set spam-log-fortiguard-response disable
     next
